    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.0",
    "eslint-plugin-vue": "^10.2.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vite-plugin-vuetify": "^2.0.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.0"
  }
