    "generate-docs": "redocly build-docs ../openapi.yaml --output public/docs/index.html"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.12.0",
    "@mdi/js": "^7.4.47",
    "axios": "^1.10.0",
    "hls.js": "^1.5.20",
    "lodash.debounce": "^4.0.8",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.0",
    "vue": "^3.5.17",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.5.0",
    "vuetify": "^3.8.11"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.77.0",
    "@redocly/cli": "^1.34.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash.debounce": "^4.0.9",
