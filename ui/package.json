    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "jiti": "^2.5.1",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  }
