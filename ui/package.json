    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.38.0",
    "eslint-plugin-vue": "^10.5.1",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^4.0.3",
    "vue-tsc": "^3.1.2"
  }
