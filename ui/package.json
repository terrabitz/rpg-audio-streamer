    "lodash.debounce": "^4.0.8",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.22",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.6.3",
    "vuetify": "^3.10.7"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.86.6",
    "@redocly/cli": "^2.8.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/lodash.debounce": "^4.0.9",
