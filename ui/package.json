    "lodash.debounce": "^4.0.8",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.27",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.6.4",
    "vuetify": "^3.11.7"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.90.10",
    "@redocly/cli": "^2.14.9",
    "@tsconfig/node22": "^22.0.5",
    "@types/jsdom": "^27.0.0",
    "@types/lodash.debounce": "^4.0.9",
