    "generate-api-client": "openapi-ts",
    "generate-docs": "redocly build-docs ../openapi.yaml --output public/docs/index.html"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.13.1",
    "@mdi/js": "^7.4.47",
    "axios": "^1.11.0",
    "hls.js": "^1.6.7",
    "lodash.debounce": "^4.0.8",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "vue": "^3.5.18",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.5.0",
    "vuetify": "^3.9.3"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.80.2",
    "@redocly/cli": "^2.0.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "1.3.4",
